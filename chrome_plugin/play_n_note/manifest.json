{
  "name": "Coursera Notes",
  "version": "0.3.5",
  "description": "Watch Videos. Take Notes. Share. All from one place.",
  "background": { 
    "scripts": [
      "jquery.js",
      "jqueryui.js",
      "oauth2/oauth2.js",
      "oauth2/oauth2_finish.js",
      "background.js",
      "shownotes.js"
    ] 
  },
 "browser_action": {
    "default_title": "",
    "default_icon": "playnnote16.png",
    "default_popup": ""
  },
  "content_scripts": [
    {
      "matches": ["http://www.google.com/robots.txt*"],
      "js": ["oauth2/oauth2_inject.js", "oauth2/oauth2_finish.js"],
        "run_at": "document_start"
    }
  ],
  "web_accessible_resources": [
    "oauth2/oauth2.html"
  ],
  "permissions" : [
    "tabs",
    "webNavigation",
    "activeTab",
    "storage",
    "http://playnnote.herokuapp.com/",
    "https://class.coursera.org/*",
    "https://ssl.gstatic.com/",
    "https://accounts.google.com/*",
    "https://www.googleapis.com/*",
    "https://www.googleapis.com/oauth2/v2/userinfo",
    "https://www.googleapis.com/auth/userinfo.profile"
  ],
  "icons" : {
    "48" : "playnnote48.png",
    "128" : "playnnote128.png"
  },
  "minimum_chrome_version": "29",
  "oauth2": {
      "client_id": "89641588136-g285g8mu99m7pc778p85rehrjkt58osb.apps.googleusercontent.com",
      "scopes": ["https://www.googleapis.com/auth/userinfo.profile"]
  },
  "manifest_version": 2
}
