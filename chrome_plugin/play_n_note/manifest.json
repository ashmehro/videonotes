{
  "name": "Coursera Notes",
  "version": "0.3.11.4",
  "description": "Watch Videos. Take Notes. Share. All from one place.",
  "background": { 
    "scripts": [
      "jquery.js",
      "jqueryui.js",
      "oauth2/oauth2.js",
      "oauth2/oauth2_finish.js",
      "background.js",
      "shownotes.js"
    ] 
  },
 "externally_connectable" : {
    "matches" : ["http://localhost:3000/*", 
                  "http://playnnote.herokuapp.com/*"
                ]
  },
 "browser_action": {
    "default_title": "",
    "default_icon": "playnnote16.png",
    "default_popup": ""
  },
  "content_scripts": [
    {
      "matches": ["http://www.google.com/robots.txt*"],
      "js": ["oauth2/oauth2_inject.js", "oauth2/oauth2_finish.js"],
        "run_at": "document_start"
    }
  ],
  "web_accessible_resources": [
    "oauth2/oauth2.html", 
    "ckeditor_basic_basic/*.js",
    "ckeditor_basic/*.css",
    "ckeditor_basic/skins/moono/*",
    "ckeditor_basic/skins/moono/images/*.png",
    "ckeditor_basic/skins/moono/images/hidpi/*.png",
    "ckeditor_basic/plugins/*",
    "ckeditor_basic/plugins/*/*",
    "ckeditor_basic/plugins/*/*/*",
    "ckeditor_basic/plugins/*/*/*/*",
    "ckeditor_basic/lang/en.js",
    "ckeditor_basic/adapters/jquery.js",
    "jquery.js",
    "jqueryui.js",
    "jquery-ui.css"
  ],
  "permissions" : [
    "tabs",
    "webNavigation",
    "activeTab",
    "storage",
    "http://localhost:3000/*",
    "https://playnnote.herokuapp.com/*",
    "http://playnnote.herokuapp.com/*",
    "https://class.coursera.org/*",
    "https://ssl.gstatic.com/",
    "https://accounts.google.com/*",
    "https://www.googleapis.com/*",
    "https://www.googleapis.com/oauth2/v2/userinfo",
    "https://www.googleapis.com/auth/userinfo.profile",
    "http://latex.codecogs.com/js/*.js",
    "http://latex.codecogs.com/css/*.css"
  ],
  "icons" : {
    "48" : "playnnote48.png",
    "128" : "playnnote128.png"
  },
  "key": "ehjngheochkknhdfjhnkpeabhikmhefj",
  "minimum_chrome_version": "29",
  "oauth2": {
      "client_id": "89641588136-902f29oc7tl588mbe0987erhf2gnoouq.apps.googleusercontent.com",
      "scopes": ["https://www.googleapis.com/auth/userinfo.profile"]
  },
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' https://playnnote.herokuapp.com https://latex.codecogs.com ; object-src 'self'"
}
